      steps {
        echo 'building Fullstack application...'
      }
    }

    stage("test") {
      steps {
        echo 'testing Fullstack application...'
      }
    }

    stage("deploy") {
      steps {
        echo 'deploying Fullstack application...'
      }
    }

  }
}
